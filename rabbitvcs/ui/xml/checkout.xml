<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="CheckoutWidget" parent="GtkGrid">
    <child>
      <object class="GtkGrid">
        <property name="margin-start">12</property>
        <property name="margin-end">12</property>
        <property name="margin_top">12</property>
        <property name="margin_bottom">12</property>
        <property name="hexpand">1</property>
        <property name="vexpand">1</property>
        <property name="orientation">vertical</property>
        <property name="row_spacing">18</property>
        <child>
          <object class="GtkGrid" id="revision_selector_box">
            <property name="hexpand">1</property>
            <property name="vexpand">1</property>
            <property name="row_spacing">6</property>
            <child>
              <object class="GtkLabel" id="label5">
                <property name="hexpand">1</property>
                <property name="label" translatable="1">&lt;b&gt;Revision&lt;/b&gt;</property>
                <property name="use_markup">1</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkGrid" id="revision_container">
                <property name="margin-start">12</property>
                <property name="hexpand">1</property>
                <property name="orientation">vertical</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <layout>
              <property name="column">0</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkGrid" id="options_box">
            <property name="hexpand">1</property>
            <property name="row_spacing">6</property>
            <child>
              <object class="GtkCheckButton" id="omit_externals">
                <property name="label" translatable="1">Omit Externals</property>
                <property name="focusable">1</property>
                <property name="halign">start</property>
                <property name="margin-start">12</property>
                <property name="hexpand">1</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="recursive">
                <property name="label" translatable="1">Recursive</property>
                <property name="focusable">1</property>
                <property name="halign">start</property>
                <property name="margin-start">12</property>
                <property name="hexpand">1</property>
                <property name="active">1</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label1">
                <property name="label" translatable="1">&lt;b&gt;Options&lt;/b&gt;</property>
                <property name="use_markup">1</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <layout>
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkGrid">
            <property name="hexpand">1</property>
            <property name="row_spacing">6</property>
            <property name="column_spacing">6</property>
            <child>
              <object class="GtkLabel" id="label4">
                <property name="margin-start">12</property>
                <property name="label" translatable="1">Destination:</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkEntry" id="destination">
                <property name="focusable">1</property>
                <property name="hexpand">1</property>
                <layout>
                  <property name="column">1</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label3">
                <property name="margin-start">12</property>
                <property name="label" translatable="1">URL:</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="file_chooser">
                <property name="focusable">1</property>
                <property name="receives_default">1</property>
                <property name="tooltip_text" translatable="1">Browse...</property>
                <child>
                  <object class="GtkImage" id="image2">
                    <property name="icon_name">drive-harddisk</property>
                  </object>
                </child>
                <layout>
                  <property name="column">2</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label2">
                <property name="label" translatable="1">&lt;b&gt;Repository&lt;/b&gt;</property>
                <property name="use_markup">1</property>
                <property name="xalign">0</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                  <property name="column-span">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkGrid">
                <property name="hexpand">1</property>
                <property name="column_spacing">6</property>
                <child>
                  <object class="GtkComboBoxText" id="repositories">
                    <property name="hexpand">1</property>
                    <property name="has_entry">1</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="repo_chooser">
                    <property name="focusable">1</property>
                    <property name="receives_default">1</property>
                    <property name="has_tooltip">1</property>
                    <property name="tooltip_text" translatable="1">Browse...</property>
                    <child>
                      <object class="GtkImage" id="image3">
                        <property name="icon_name">edit-find</property>
                      </object>
                    </child>
                    <layout>
                      <property name="column">1</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <layout>
                  <property name="column">1</property>
                  <property name="row">1</property>
                  <property name="column-span">2</property>
                </layout>
              </object>
            </child>
            <layout>
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
