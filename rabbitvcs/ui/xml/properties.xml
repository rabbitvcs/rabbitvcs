<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="PropertiesWidget" parent="GtkGrid">
    <property name="margin-start">12</property>
    <property name="margin-end">12</property>
    <property name="margin_top">12</property>
    <property name="margin_bottom">12</property>
    <property name="hexpand">1</property>
    <property name="vexpand">1</property>
    <property name="orientation">vertical</property>
    <property name="row_spacing">18</property>
    <child>
      <object class="GtkGrid">
        <property name="hexpand">1</property>
        <property name="vexpand">1</property>
        <property name="orientation">vertical</property>
        <property name="row_spacing">6</property>
        <child>
          <object class="GtkGrid">
            <property name="margin-start">12</property>
            <property name="hexpand">1</property>
            <property name="vexpand">1</property>
            <property name="orientation">vertical</property>
            <property name="row_spacing">6</property>
            <child>
              <object class="GtkBox" id="hbuttonbox9">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="margin_top">8</property>
                <property name="hexpand">True</property>
                <property name="spacing">3</property>
                <child>
                  <object class="GtkButton" id="new">
                    <property name="label" translatable="1">_New...</property>
                    <property name="focusable">1</property>
                    <property name="receives_default">1</property>
                    <property name="use_underline">1</property>
                    <child>
                      <object class="GtkImage">
                        <property name="icon_name">document-new</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="edit">
                    <property name="label" translatable="1">_Edit...</property>
                    <property name="sensitive">0</property>
                    <property name="focusable">1</property>
                    <property name="receives_default">1</property>
                    <property name="use_underline">1</property>
                    <child>
                      <object class="GtkImage">
                        <property name="icon_name">rabbitvcs-editprops</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="delete">
                    <property name="label" translatable="1">_Delete</property>
                    <property name="sensitive">0</property>
                    <property name="focusable">1</property>
                    <property name="receives_default">1</property>
                    <property name="use_underline">1</property>
                    <child>
                      <object class="GtkImage">
                        <property name="icon_name">edit-delete</property>
                      </object>
                    </child>
                  </object>
                </child>
                <layout>
                  <property name="column">0</property>
                  <property name="row">4</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="delete_recurse">
                <property name="label" translatable="1">Delete properties recursively</property>
                <property name="focusable">1</property>
                <property name="halign">start</property>
                <property name="hexpand">1</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="recursive_label">
                <property name="halign">start</property>
                <property name="hexpand">1</property>
                <property name="label" translatable="1">&lt;i&gt;Selected properties will be applied recursively.&lt;/i&gt;</property>
                <property name="use_markup">1</property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">2</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkScrolledWindow" id="scrolledwindow12">
                <property name="height_request">150</property>
                <property name="focusable">1</property>
                <property name="hexpand">1</property>
                <property name="vexpand">1</property>
                <property name="child">
                  <object class="GtkTreeView" id="table">
                    <property name="focusable">1</property>
                    <property name="rubber_banding">1</property>
                    <child internal-child="selection">
                      <object class="GtkTreeSelection"/>
                    </child>
                  </object>
                </property>
                <layout>
                  <property name="column">0</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkGrid">
                <property name="hexpand">1</property>
                <property name="column_spacing">6</property>
                <child>
                  <object class="GtkButton" id="refresh">
                    <property name="focusable">1</property>
                    <property name="receives_default">1</property>
                    <child>
                      <object class="GtkImage" id="image1">
                        <property name="icon_name">view-refresh</property>
                      </object>
                    </child>
                    <layout>
                      <property name="column">1</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkEntry" id="path">
                    <property name="focusable">1</property>
                    <property name="tooltip_text">URL/Path</property>
                    <property name="hexpand">1</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <layout>
                  <property name="column">0</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <layout>
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="label1">
            <property name="label" translatable="1">&lt;b&gt;Properties for:&lt;/b&gt;</property>
            <property name="use_markup">1</property>
            <property name="xalign">0</property>
            <layout>
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <layout>
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
  </template>
</interface>
