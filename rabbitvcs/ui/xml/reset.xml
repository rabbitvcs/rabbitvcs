<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="ResetWidget" parent="GtkBox">
    <property name="margin-start">12</property>
    <property name="margin-end">12</property>
    <property name="margin_top">12</property>
    <property name="margin_bottom">12</property>
    <property name="hexpand">1</property>
    <property name="vexpand">1</property>
    <property name="orientation">vertical</property>
    <property name="spacing">18</property>
    <child>
      <object class="GtkBox">
        <property name="hexpand">1</property>
        <property name="orientation">vertical</property>
        <property name="spacing">6</property>
        <child>
          <object class="GtkLabel" id="label1">
            <property name="hexpand">1</property>
            <property name="label" translatable="1">&lt;b&gt;Reset your Repository&lt;/b&gt;</property>
            <property name="use_markup">1</property>
            <property name="xalign">0</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="label3">
            <property name="hexpand">1</property>
            <property name="label" translatable="1">Reset current HEAD to specified state.</property>
            <property name="xalign">0</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkBox">
        <property name="hexpand">1</property>
        <property name="orientation">vertical</property>
        <property name="spacing">6</property>
        <child>
          <object class="GtkLabel" id="label9">
            <property name="hexpand">1</property>
            <property name="label" translatable="1">&lt;b&gt;Path&lt;/b&gt;</property>
            <property name="use_markup">1</property>
            <property name="xalign">0</property>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="spacing">4</property>
            <child>
              <object class="GtkEntry" id="path">
                <property name="focusable">1</property>
                <property name="margin-start">12</property>
                <property name="hexpand">1</property>
                <property name="invisible_char">‚óè</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="browse">
                <property name="focusable">1</property>
                <property name="receives_default">1</property>
                <child>
                  <object class="GtkImage" id="image1">
                    <property name="icon_name">drive-harddisk</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkBox">
        <property name="hexpand">1</property>
        <property name="vexpand">0</property>
        <property name="orientation">vertical</property>
        <property name="spacing">6</property>
        <child>
          <object class="GtkLabel" id="label8">
            <property name="label" translatable="1">&lt;b&gt;Revision&lt;/b&gt;</property>
            <property name="use_markup">1</property>
            <property name="xalign">0</property>
          </object>
        </child>
        <child>
          <object class="GtkGrid" id="revision_container">
            <property name="margin-start">12</property>
            <property name="hexpand">1</property>
            <property name="vexpand">0</property>
            <property name="orientation">vertical</property>
            <child>
              <placeholder/>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkBox">
        <property name="hexpand">1</property>
        <property name="orientation">vertical</property>
        <property name="spacing">6</property>
        <child>
          <object class="GtkLabel" id="label2">
            <property name="label" translatable="1">&lt;b&gt;Options&lt;/b&gt;</property>
            <property name="use_markup">1</property>
            <property name="xalign">0</property>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="margin-start">12</property>
            <property name="hexpand">1</property>
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkCheckButton" id="mixed_opt">
                <property name="halign">start</property>
                <property name="hexpand">1</property>
                <child>
                  <object class="GtkLabel" id="label4">
                    <property name="label" translatable="1">&lt;i&gt;Mixed&lt;/i&gt; - Reset the index but not the working tree</property>
                    <property name="use_markup">1</property>
                    <property name="xalign">0</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="soft_opt">
                <property name="halign">start</property>
                <property name="hexpand">1</property>
                <property name="group">mixed_opt</property>
                <child>
                  <object class="GtkLabel" id="label5">
                    <property name="label" translatable="1">&lt;i&gt;Soft&lt;/i&gt; - Does not touch the index file or working tree at all, 
but requires them to be in good working order</property>
                    <property name="use_markup">1</property>
                    <property name="xalign">0</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="hard_opt">
                <property name="halign">start</property>
                <property name="hexpand">1</property>
                <property name="group">mixed_opt</property>
                <child>
                  <object class="GtkLabel" id="label6">
                    <property name="label" translatable="1">&lt;i&gt;Hard&lt;/i&gt; - Matches the working tree and index to that of the 
tree being switched to</property>
                    <property name="use_markup">1</property>
                    <property name="xalign">0</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="merge_opt">
                <property name="halign">start</property>
                <property name="hexpand">1</property>
                <property name="group">mixed_opt</property>
                <child>
                  <object class="GtkLabel" id="label7">
                    <property name="label" translatable="1">&lt;i&gt;Merge&lt;/i&gt; - Resets the index to match the tree recorded by the named commit,
and updates the files that are different between the named commit
and the current commit in the working tree</property>
                    <property name="use_markup">1</property>
                    <property name="xalign">0</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="none_opt">
                <property name="halign">start</property>
                <property name="hexpand">1</property>
                <property name="active">1</property>
                <property name="group">mixed_opt</property>
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="1">None</property>
                    <property name="xalign">0</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
