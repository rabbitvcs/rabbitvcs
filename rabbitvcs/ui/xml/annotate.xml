<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="AnnotateWidget" parent="GtkGrid">
    <property name="margin-start">12</property>
    <property name="margin-end">12</property>
    <property name="margin_top">12</property>
    <property name="margin_bottom">12</property>
    <property name="hexpand">1</property>
    <property name="vexpand">1</property>
    <property name="orientation">vertical</property>
    <property name="row_spacing">18</property>
    <child>
      <object class="GtkGrid">
      <property name="column_spacing">6</property>
        <property name="hexpand">1</property>
        <child>
          <object class="GtkEntry" id="revision">
            <property name="width_request">100</property>
            <property name="focusable">1</property>
            <property name="hexpand">1</property>
            <!-- TODO <signal name="focus-out-event" handler="on_revision_focus_out_event" swapped="no"/>
            <signal name="key-press-event" handler="on_revision_key_press_event" swapped="no"/> -->
            <layout>
              <property name="column">2</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="show_log">
            <property name="focusable">1</property>
            <property name="receives_default">1</property>
            <child>
              <object class="GtkImage" id="image1">
                <property name="icon_name">rabbitvcs-show_log</property>
              </object>
            </child>
            <layout>
              <property name="column">3</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="label2">
            <property name="label" translatable="1">&lt;b&gt;Revision&lt;/b&gt;</property>
            <property name="use_markup">1</property>
            <property name="xalign">0</property>
            <layout>
              <property name="column">2</property>
              <property name="row">0</property>
              <property name="column-span">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="history_first">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="margin_end">4</property>
            <child>
              <object class="GtkImage">
                <property name="icon_name">go-first</property>
              </object>
            </child>
            <layout>
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="history_prev">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <child>
              <object class="GtkImage">
                <property name="icon_name">go-previous</property>
              </object>
            </child>
            <layout>
              <property name="column">1</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="history_next">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="margin_end">4</property>
            <child>
              <object class="GtkImage">
                <property name="icon_name">go-next</property>
              </object>
            </child>
            <layout>
              <property name="column">4</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="history_last">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="margin_end">24</property>
            <child>
              <object class="GtkImage">
                <property name="icon_name">go-last</property>
              </object>
            </child>
            <layout>
              <property name="column">5</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <layout>
          <property name="column">0</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkScrolledWindow" id="scrolledwindow1">
        <property name="focusable">1</property>
        <property name="hexpand">1</property>
        <property name="vexpand">1</property>
        <property name="child">
          <object class="GtkTreeView" id="table">
            <property name="focusable">1</property>
            <property name="rubber_banding">1</property>
            <child internal-child="selection">
              <object class="GtkTreeSelection"/>
            </child>
          </object>
        </property>
        <layout>
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
  </template>
</interface>
